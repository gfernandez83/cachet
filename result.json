.mainContainer{
    margin: auto;
}

.parallax-bx .stay-bx .social li a {
    background: none !important;
    display: inline;
    width: auto;
    height: auto;
}

.hero .slick-arrow{
    display: none !important;
}
.hero .slick-dots{
    display: block !important;
}
.hero .slick-dots li {
    margin: 0 4px;
}
.hero .slick-dots li button {
    width: 16px;
    height: 16px;
}
.scroll-top {
    bottom: 45px;
}

#footer{
    background: #312d2a;
}
#footer #footer-infos{
    background: #312d2a url(/uploaded/cf8a48f6d4c0ea3c1cca2f2635385cb9_5664eba86bcbc.jpeg) no-repeat center bottom;
    background-size: contain;
}

/* Progressive Jackpot */
#progJackpot {
    padding-top: 0;
    margin: 3px 0 -3px 0;
}
.home-full {
    padding-left: 5px;
    padding-right: 5px;
}
.home-full .game-box{
    line-height: 0;
}

/* announcement vertical centered modal */
#notification-window {
    display: none;
}
.vertical-alignment-helper {
    display:table;
    height: 100%;
    width: 100%;
    pointer-events:none;
}
.vertical-align-center {
    display: table-cell;
    vertical-align: middle;
    pointer-events:none;
}
#notification-modal .modal-content {
    width:inherit;
    height:inherit;
    margin: 0 auto;
    pointer-events:all;
}

/* new footer links */
.footer-payment-links a{
    background: url(/uploaded/9deac24cd910e0948bacbb0a5a861979_567273b7598a9.png) no-repeat transparent;
}
.footer-payment-links a{
    height: 24px;
    display: block;
}
.footer-payment-links a.pm-neteller {
    width: 144px;
    background-position: 0 -24px;
}
.footer-payment-links a.pm-neteller:hover {
    background-position: 0 0;
}
.footer-payment-links a.pm-unionpay {
    width: 39px;
    background-position: -152px -25px;
}
.footer-payment-links a.pm-unionpay:hover {
    background-position: -152px 0;
}
.footer-payment-links a.pm-alipay {
    width: 66px;
    background-position: -197px -24px;
}
.footer-payment-links a.pm-alipay:hover {
    background-position: -197px 0;
}
.social a, .browser a, .cc-links a{
    width: 35px;
    height: 35px;
    display: block;
}
.browser a, .cc-links a{
    background: url(/uploaded/146ce7e574f5dace1760db23e6d59ca0_567273b08f7b8.png) no-repeat transparent;
}
.social a{
    background: url(/uploaded/2a5120efd5a50c7763b7fc1dc3370fa9_567273aecc129.png) no-repeat transparent;
}
.social a:hover, .footer-payment-links a img:hover, .cc-links a:hover{
    -webkit-filter: grayscale(0);
}
.social a.social-skype{
    background-position: 0 -35px;
}
.social a.social-skype:hover{
    background-position: 0 0;
}
.social a.social-weibo{
    background-position: -49px -35px;
    width: 42px;
}
.social a.social-weibo:hover{
    background-position: -49px 0;
}
.social a.social-qq{
    background-position: -105px -35px;
}
.social a.social-qq:hover{
    background-position: -105px 0;
}
.social a.social-envelope{
    background-position: -150px -35px;
    width: 40px;
}
.social a.social-envelope:hover{
    background-position: -150px 0;
}
.social a.social-uu-footer-logo{
    background-position: -204px -35px;
}
.social a.social-uu-footer-logo:hover{
    background-position: -204px 0;
}
.browser a:hover {
    -webkit-filter: grayscale(0);
}
.browser a.ie {
    background-position: 1px -33px;
}
.browser a.ie:hover {
    background-position: 1px 1px;
}
.browser a.chrome {
    background-position: -48px -34px;
}
.browser a.chrome:hover {
    background-position: -48px 0;
}
.browser a.mozilla {
    background-position: -97px -33px;
}
.browser a.mozilla:hover {
    background-position: -97px 1px;
}
.browser a.sogou {
    background-position: -148px -34px;
}
.browser a.sogou:hover {
    background-position: -148px 0;
}
.cc-links a.clear-cache {
    background-position: -197px -34px;
}
.cc-links a.clear-cache:hover {
    background-position: -197px 0;
}
.cc-links a.teamviewer {
    background-position: -243px -34px;
}
.cc-links a.teamviewer:hover {
    background-position: -243px 0;
}

.jackpot-animated {
    margin: 5px -10px auto;
}
.display-table {
    margin: 0 auto;
    display: table;
}

@media (max-width:1199px) {
    .fixed-header, #mystique {
        padding-top: 132px;
    }
    .header2 .navbar .header-box .navbar-brand img {
        max-height: 56px;
    }
}

/* Media Queries */
@media (max-width:991px) {
    .jackpot-animated .text-left {
        padding-top: 5px !important;
    }
}
@media (max-width:767px) {
    .jackpot-animated .jackpot-label,
    .jackpot-animated .text-left {
        text-align: center;
    }
    .jackpot-animated .jackpot-label img {
        display: inline-block;
    }
}

.deposit-message-wrapper {
    padding: 15px;
    background-color: #eee;
    margin-bottom: 10px;
}

@media only screen and (max-width: 320px) {
    .fixed-header, #mystique {
        padding-top: 0;
    }    
}




